{
  "name": "node-server-with-typescript",
  "version": "1.0.0",
  "description": "A Web & API server built using TypeScript",
  "main": "dist",
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.7",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.21.1",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.11",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.5",
    "sequelize-typescript": "^2.1.6",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^12.20.55",
    "@types/sequelize": "^4.28.20",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "typescript": "5.0.3",
    "typescript-eslint-parser": "^18.0.0"
  },
  "scripts": {
    "build": "npm run tsc",
    "tsc": "tsc -p tsconfig.json",
    "dev": "tsc --watch & NODE_ENV=development nodemon dist",
    "test": "tsc && ./node_modules/.bin/mocha dist/**/*.spec.js",
    "lint": "eslint . --ext .ts",
    "clean": "rm -rf dist/ .logs/",
    "start": "node dist",
    "migration:create": "npx sequelize-cli migration:generate --name",
    "migration:up": "npx sequelize-cli db:migrate --config sequelize-cli.js",
    "migration:down": "npx sequelize-cli db:migrate:undo --config sequelize-cli.js",
    "seed:create": "npx sequelize-cli seed:generate --name",
    "seed:up": "npx sequelize-cli db:seed:all --config sequelize-cli.js",
    "seed:down": "npx sequelize-cli db:seed:undo --config sequelize-cli.js"
  },
  "license": "ISC"
}
